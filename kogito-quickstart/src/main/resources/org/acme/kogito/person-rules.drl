package org.acme.kogito

import org.acme.kogito.model.Person;


declare ContactoInputOutputModel
   input : ContactoInputModel
   output : ContactoOutputModel
end


rule "Is adult" ruleflow-group "person"

when
    $person: Person(age > 18)
then
    modify($person) { 
    	setAdult(true) 
    };
end
